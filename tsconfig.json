{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
    "removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "ES6",
		"moduleResolution": "NodeNext",
    "plugins": [
      {
        "name": "typescript-svelte-plugin",
        "enabled": true,
        "assumeIsSvelteProject": true
      }
    ]
	}
}
